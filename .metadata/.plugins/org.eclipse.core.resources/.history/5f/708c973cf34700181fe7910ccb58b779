package itteration;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;

import javax.swing.plaf.SliderUI;

import bean.Student;

public class AllIterator {
	
	public static void main(String[] args) {
		
		Student s1=new Student(1, "Swapnil", "Pune");
		Student s2=new Student(2, "Sahil", "Karad");
		Student s3=new Student(3, "Saket", "Mumbai");
		
		List<Student> sList = new ArrayList<>();
		
		sList.add(s1);
		sList.add(s2);
		sList.add(s3);
		
		Iterator<Student> studentIterator = sList.iterator();
		listIterator(studentIterator);
		
	}
	
	private static void listIterator(Iterator<Student> studentIterator){
		while(studentIterator.hasNext()){
			System.out.println(studentIterator.next());
		}
	}
	

}
